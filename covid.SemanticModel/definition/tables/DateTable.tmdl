	partition DateTable = calculated
		mode: import
		source =
				
				VAR MinDate = MIN( 'cases_deaths'[date] )
				VAR MaxDate = MAX( 'cases_deaths'[date] )
				RETURN
				ADDCOLUMNS(
				    CALENDAR( MinDate, MaxDate ),
				    "Year", YEAR( [Date] ),
				    "MonthNumber", MONTH( [Date] ),
				    "MonthName", FORMAT( [Date], "MMM" ),
				    "Quarter", "Q" & CEILING( MONTH( [Date] ) / 3, 1 ),
				    "DayOfWeek", WEEKDAY( [Date], 2 ), // 2 for Monday as the first day of the week
				    "DayName", FORMAT( [Date], "DDD" ),
				    "WeekEnding", [Date] - WEEKDAY( [Date], 2 ) + 7 // Calculate the end of the week (Sunday)
				)

	annotation PBI_Id = 26069cabad054a1ab1ddec825d87e5d5

